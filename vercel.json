{
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "vercel-python-wsgi"
    }
  ],
  "routes": [
    { "src": "/webhook", "methods": ["POST"], "dest": "api/wsgi.py" },
    { "src": "/set_webhook", "methods": ["GET"], "dest": "api/wsgi.py" },
    { "src": "/", "methods": ["GET"], "dest": "api/wsgi.py" }
  ]
}