{
  "builds": [
    {
      "src": "api/archivo.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "api/archivo.py"
    },
    {
      "src": "/set_webhook",
      "methods": ["GET"],
      "dest": "api/archivo.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/archivo.py"
    }
  ]
}